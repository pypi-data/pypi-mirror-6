<html tal:replace="structure context/manage_page_header"/>

    <div tal:replace="structure python:context.manage_form_title(context, context,
           form_title='<img src=%s/misc_/BastionLedger/bltaxtablerec> %s' % (request['BASEPATH1'],
'Add Tax Table Record'),
           help_product='BastionLedger',
           help_topic='add.stx')"/>

     <form action="manage_addBLTaxTableRecord" method="post">
          <table cellspacing="0" cellpadding="2" border="0">
              <tr>
                  <td class="form-label">Rate</td>
                  <td class="form-element">
                      <input type="text" name="rate:float" size="20">
                  </td>
              </tr>
              <tr>
                  <td class="form-label">Effective</td>
                  <td class="form-element">
                      <input type="text" name="effective:date" size="20">
                  </td>
              </tr>
              <tr tal:repeat="kw context/dimensions">
                  <td class="form-label" tal:content="python: kw.capitalize()"/>
                  <td class="form-element">
                      <input type="text" name="" size="20"
                             tal:attributes="name python:'kw:%s:%s:record' % (kw, context.fieldType(kw))"/>
                  </td>
              </tr>
              <tr>
                  <td class="form-element">
                     <input type="submit" value=" Add ">
       		  </td>
              </tr>
          </table>
     </form>
   
<html tal:replace="structure context/manage_page_footer"/>
