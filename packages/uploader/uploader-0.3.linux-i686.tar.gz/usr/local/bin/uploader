#!/usr/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'uploader==0.3','console_scripts','uploader'
__requires__ = 'uploader==0.3'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('uploader==0.3', 'console_scripts', 'uploader')()
    )
