<html tal:replace="structure context/manage_page_header"/>
     <span tal:replace="structure context/manage_tabs"/>

      <br/>

      <form action="" method="post" tal:attributes="action request/URL1">
         <table cellspacing="0" cellpadding="2" border="0">
           <tr>
               <td class="form-label">Code</td>
               <td class="form-element">
                  <input type="text" name="code" size="5"
                         value="" tal:attributes="value context/code"/>
               </td>
           </tr>
           <tr>
               <td class="form-label">Effective Date</td>
               <td class="form-element">
                  <input type="text" name="effective:date" size="20"
                         value="" tal:attributes="value context/effective"/>
               </td>
           </tr>
           <tr>
               <td class="form-label">Rate</td>
               <td class="form-element">
                  <input type="text" name="rate:float" size="5"
                         tal:attributes="value context/rate"/>
               </td>
           </tr>
           <tr tal:repeat="d context/extras">
               <td class="form-label" tal:content="python: d.capitalize()"/>
               <td class="form-element"
                   tal:define="value python: getattr(context, d, '')">
                      <input type="text" name="" size="20"
                             tal:attributes="name python:'kw.%s:%s:record' % (d, context.fieldType(d));
                                             value value"/>
               </td>
           </tr>
         </table>
         <input type="submit" name="manage_edit:method" value=" Edit "/>
      </form>

<html tal:replace="structure context/manage_page_footer"/>




