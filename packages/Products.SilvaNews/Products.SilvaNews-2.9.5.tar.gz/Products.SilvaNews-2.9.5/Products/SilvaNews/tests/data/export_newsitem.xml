<?xml version="1.0" encoding="utf-8"?>
<silva xmlns="http://infrae.com/namespace/silva"
  {namespaces} silva_version="{version}">
  <folder id="export">
    <metadata>
      <set id="silva-content">
        <silva-content:maintitle>Export Folder</silva-content:maintitle>
        <silva-content:shorttitle/>
      </set>
      <set id="silva-extra">
        <silva-extra:comment/>
        <silva-extra:contactemail/>
        <silva-extra:contactname/>
        <silva-extra:content_description/>
        <silva-extra:creationtime>YYYY-MM-DDTHH:MM:SS</silva-extra:creationtime>
        <silva-extra:creator>author</silva-extra:creator>
        <silva-extra:expirationtime/>
        <silva-extra:hide_from_tocs>do not hide</silva-extra:hide_from_tocs>
        <silva-extra:keywords/>
        <silva-extra:language/>
        <silva-extra:lastauthor>author</silva-extra:lastauthor>
        <silva-extra:modificationtime>YYYY-MM-DDTHH:MM:SS</silva-extra:modificationtime>
        <silva-extra:publicationtime/>
        <silva-extra:subject/>
      </set>
    </metadata>
    <content>
      <silvanews:newspublication id="newspub">
        <metadata>
          <set id="silva-content">
            <silva-content:maintitle>NewsPublication</silva-content:maintitle>
            <silva-content:shorttitle/>
          </set>
          <set id="silva-extra">
            <silva-extra:comment/>
            <silva-extra:contactemail/>
            <silva-extra:contactname/>
            <silva-extra:content_description/>
            <silva-extra:creationtime>YYYY-MM-DDTHH:MM:SS</silva-extra:creationtime>
            <silva-extra:creator>author</silva-extra:creator>
            <silva-extra:expirationtime/>
            <silva-extra:hide_from_tocs>hide</silva-extra:hide_from_tocs>
            <silva-extra:keywords/>
            <silva-extra:language/>
            <silva-extra:lastauthor>author</silva-extra:lastauthor>
            <silva-extra:modificationtime>YYYY-MM-DDTHH:MM:SS</silva-extra:modificationtime>
            <silva-extra:publicationtime/>
            <silva-extra:subject/>
          </set>
          <set xmlns:snn-np-settings="http://infrae.com/namespace/metadata/snn-np-settings" id="snn-np-settings">
            <snn-np-settings:is_private>no</snn-np-settings:is_private>
          </set>
        </metadata>
        <content>
          <default>
            <auto_toc depth="-1" display_desc_flag="False" id="index" show_icon="False" sort_order="silva" types="Silva Article,Silva Agenda Item">
              <metadata>
                <set id="silva-content">
                  <silva-content:maintitle>NewsPublication</silva-content:maintitle>
                  <silva-content:shorttitle/>
                </set>
                <set id="silva-extra">
                  <silva-extra:comment/>
                  <silva-extra:contactemail/>
                  <silva-extra:contactname/>
                  <silva-extra:content_description/>
                  <silva-extra:creationtime>YYYY-MM-DDTHH:MM:SS</silva-extra:creationtime>
                  <silva-extra:creator>author</silva-extra:creator>
                  <silva-extra:expirationtime/>
                  <silva-extra:hide_from_tocs>do not hide</silva-extra:hide_from_tocs>
                  <silva-extra:keywords/>
                  <silva-extra:language/>
                  <silva-extra:lastauthor>author</silva-extra:lastauthor>
                  <silva-extra:modificationtime>YYYY-MM-DDTHH:MM:SS</silva-extra:modificationtime>
                  <silva-extra:publicationtime/>
                  <silva-extra:subject/>
                </set>
              </metadata>
            </auto_toc>
          </default>
          <silvanews:plainarticle id="news">
            <workflow>
              <version id="0">
                <status>unapproved</status>
                <publication_datetime/>
                <expiration_datetime/>
              </version>
            </workflow>
            <content
              display_datetime="YYYY-MM-DDTHH:MM:SS"
              subjects="all"
              target_audiences="generic"
              version_id="0">
              <metadata>
                <set id="silva-content">
                  <silva-content:maintitle>Some news</silva-content:maintitle>
                  <silva-content:shorttitle/>
                </set>
                <set id="silva-extra">
                  <silva-extra:comment/>
                  <silva-extra:contactemail/>
                  <silva-extra:contactname/>
                  <silva-extra:content_description/>
                  <silva-extra:creationtime>YYYY-MM-DDTHH:MM:SS</silva-extra:creationtime>
                  <silva-extra:creator>author</silva-extra:creator>
                  <silva-extra:expirationtime/>
                  <silva-extra:hide_from_tocs>do not hide</silva-extra:hide_from_tocs>
                  <silva-extra:keywords/>
                  <silva-extra:language/>
                  <silva-extra:lastauthor>author</silva-extra:lastauthor>
                  <silva-extra:modificationtime>YYYY-MM-DDTHH:MM:SS</silva-extra:modificationtime>
                  <silva-extra:publicationtime/>
                  <silva-extra:subject/>
                </set>
              </metadata>
              <doc:doc/>
            </content>
          </silvanews:plainarticle>
        </content>
      </silvanews:newspublication>
    </content>
  </folder>
</silva>
