# EASY-INSTALL-ENTRY-SCRIPT: 'format-sql==0.0.1','console_scripts','format-sql'
__requires__ = 'format-sql==0.0.1'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('format-sql==0.0.1', 'console_scripts', 'format-sql')()
    )
