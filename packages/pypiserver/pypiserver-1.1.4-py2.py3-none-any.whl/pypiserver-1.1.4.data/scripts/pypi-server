#!python
# EASY-INSTALL-ENTRY-SCRIPT: 'pypiserver==1.1.4','console_scripts','pypi-server'
__requires__ = 'pypiserver==1.1.4'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('pypiserver==1.1.4', 'console_scripts', 'pypi-server')()
    )
