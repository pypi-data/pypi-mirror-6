<?xml version="1.0" encoding="iso-8859-1"?>

<form>
  <title>Upload</title>
  <name>uploadform</name>
  <action></action>
  <enctype></enctype>
  <method>POST</method>

  <i18n_domain>silva_external_sources</i18n_domain>
  <i18n>default_edit</i18n>

  <groups>
    <group>
      <title>Default</title>
      <fields>
      
      <field><id>file</id> <type>FileField</type>
        <values>
          <alternate_name></alternate_name>
          <css_class></css_class>
          <default></default>
          <description>File to upload as source data.</description>
          <display_maxwidth></display_maxwidth>
          <display_width type="int">40</display_width>
          <external_validator></external_validator>
          <extra></extra>
          <required type="int">0</required>
          <hidden type="int">0</hidden>
          <title>csv file</title>
        </values>
        <tales>
        </tales>
        <messages>
          <message name="external_validator_failed">The input failed the external validator.</message>
        </messages>
      </field>
      </fields>
    </group>
  </groups>
</form>
