<?xml version="1.0"?>

<form>
  <title>Parameters form</title>
  <row_length>4</row_length>
  <name>form</name>
  <action></action>
  <method>POST</method>
  <enctype></enctype>
  <encoding>UTF-8</encoding>
  <stored_encoding>ISO-8859-1</stored_encoding>
  <unicode_mode>true</unicode_mode>
  <i18n_domain></i18n_domain>
  <groups>
    <group>
      <title>Default</title>
      <fields>
        
        <field><id>url</id> <type>LookupWindowField</type>
        <values>
          <alternate_name></alternate_name>
          <css_class>store</css_class>
          <default></default>
          <description>Location of Flash file to embed.</description>
          <display_maxwidth></display_maxwidth>
          <display_width type="int">20</display_width>
          <enabled type="int">1</enabled>
          <external_validator></external_validator>
          <extra></extra>
          <hidden type="int">0</hidden>
          <max_length></max_length>
          <onclick>reference.getReference(
          function(path, id, title) {
          document.getElementById('%(field_id)s').value = path;;
          }, '%(url)s', '', true)</onclick>
          <required type="int">1</required>
          <title>url</title>
          <truncate type="int">0</truncate>
          <unicode type="int">0</unicode>
          <whitespace_preserve type="int">0</whitespace_preserve>
        </values>
        <tales>
        </tales>
        <messages>
          <message name="external_validator_failed">The input failed the external validator.</message>
          <message name="required_not_found">Input is required but no input given.</message>
          <message name="too_long">Too much input was given.</message>
        </messages>
        </field>
        <field><id>type</id> <type>RadioField</type>
        <values>
          <alternate_name></alternate_name>
          <css_class></css_class>
          <default>swf</default>
          <description>Choose the movie type.</description>
          <enabled type="int">1</enabled>
          <external_validator></external_validator>
          <extra_item></extra_item>
          <first_item type="int">1</first_item>
          <hidden type="int">0</hidden>
          <items type="list">[(u'swf', u'swf'), (u'flv', u'flv')]</items>
          <orientation>horizontal</orientation>
          <required type="int">0</required>
          <title>type</title>
          <unicode type="int">0</unicode>
          <whitespace_preserve type="int">0</whitespace_preserve>
        </values>
