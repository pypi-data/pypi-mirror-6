pi:
  description: Principal Investigator
  type: str
analyst:
  description: Responsible Bioinformatician
  type: str
xref:
  description: Database cross reference
  type: str
  cardinality: '+'
project:
  description: Associated project
  type: str
organism:
  description: Organism
  type: str
status:
  description: Status
  type: restricted
  cardinality: '1'
  allowed:
    delete: can be deleted
    trash: move to trash disks
    temp: temporary data - will be deleted in a month
    work: currently being worked on
    backup: to be backed up
    offsite: to be backed up - offsite
    archive: to be archived and deleted afterwards
backup:
  description: This data MUST be backed up
