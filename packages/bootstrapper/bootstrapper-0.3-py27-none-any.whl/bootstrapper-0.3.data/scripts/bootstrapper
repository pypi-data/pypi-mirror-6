#!python
# EASY-INSTALL-ENTRY-SCRIPT: 'bootstrapper==0.3','console_scripts','bootstrapper'
__requires__ = 'bootstrapper==0.3'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('bootstrapper==0.3', 'console_scripts', 'bootstrapper')()
    )
