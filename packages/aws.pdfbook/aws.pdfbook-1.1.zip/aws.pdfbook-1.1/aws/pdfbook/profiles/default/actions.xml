<?xml version="1.0"?>
<object name="portal_actions"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <object name="document_actions">
  <object name="pdf" meta_type="CMF Action" i18n:domain="aws.pdfbook">
   <property name="title" i18n:translate="">PDF</property>
   <property name="description"
      i18n:translate="">Download this page in PDF.</property>
   <property
      name="url_expr">string:${object_url}/@@aws-content-pdfbook</property>
   <property name="icon_expr">string:pdf_icon.gif</property>
   <property name="available_expr">object/@@aws.pdfbook.enabled</property>
   <property name="permissions">
    <element value="View"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
</object>
