<?xml version="1.0" encoding="iso-8859-1"?>

<form>
  <title>Data edit</title>
  <name>txt</name>
  <action></action>
  <enctype></enctype>
  <method>POST</method>

  <i18n_domain>silva_external_sources</i18n_domain>
  <i18n>default_edit</i18n>

  <groups>
    <group>
      <title>Default</title>
      <fields>

      <field><id>csv_rawdata</id> <type>TextAreaField</type>
        <values>
          <alternate_name></alternate_name>
          <css_class>store</css_class>
          <default></default>
          <description></description>
          <external_validator></external_validator>
          <extra>style="font-family:monospace; font-size:1.3em" </extra>
          <height type="int">25</height>
          <hidden type="int">0</hidden>
          <max_length></max_length>
          <max_linelength></max_linelength>
          <max_lines></max_lines>
          <required type="int">0</required>
          <title>data</title>
          <unicode type="int">1</unicode>
          <whitespace_preserve type="int">1</whitespace_preserve>
          <width type="int">80</width>
        </values>
        <tales>
          <default>python:form.REQUEST.model.raw_data()</default>
        </tales>
        <messages>
          <message name="external_validator_failed">The input failed the external validator.</message>
          <message name="required_not_found">Input is required but no input given.</message>
          <message name="too_many_lines">You entered too many lines.</message>
          <message name="line_too_long">A line was too long.</message>
          <message name="too_long">You entered too many characters.</message>
        </messages>
      </field>
      </fields>
    </group>
  </groups>
</form>
