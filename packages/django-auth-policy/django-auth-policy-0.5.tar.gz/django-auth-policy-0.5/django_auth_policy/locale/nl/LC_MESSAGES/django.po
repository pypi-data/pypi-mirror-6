#: settings.py:18
#, python-brace-format
msgid "One is required to change passwords every {age} days."
msgstr "Men is verplicht om elke {age} dagen het wachtwoord aan te passen."

#: settings.py:56
#, python-brace-format
msgid "Passwords must be at least {length} characters in length."
msgstr "Wachtwoorden moeten minimaal {length} tekens lang zijn."

#: settings.py:69
msgid "lowercase characters"
msgstr "kleine letters"

#: settings.py:72
msgid "at least one lowercase character"
msgstr "minimaal één kleine letter"

#: settings.py:74
msgid "uppercase characters"
msgstr "hoofdletters"

#: settings.py:77
msgid "at least one uppercase character"
msgstr "minimaal één hoofdletter"

#: settings.py:79
msgid "numbers"
msgstr "cijfers"

#: settings.py:82
msgid "at least one number"
msgstr "minimaal één cijfer"

#: settings.py:84
msgid "symbols"
msgstr "symbolen"

#: settings.py:87
msgid "at least one symbol"
msgstr "minimaal één symbool"

#: settings.py:92
#, python-brace-format
msgid "Passwords must have {rule_text}."
msgstr "Wachtwoorden moeten {rule_text} hebben."

#: user_admin.py:29
#: templates/admin/django_auth_policy/set_temporary_password.html:19
msgid "Username"
msgstr "Gebruikersnaam"

#: user_admin.py:31
msgid "Required. 30 characters or fewer. Letters, digits and @/./+/-/_ only."
msgstr ""
"Verplicht. 30 tekens of minder. Alleen letters, cijfers en de tekens @/./+/-/"
"_."

#: user_admin.py:35
msgid "This value may contain only letters, numbers and @/./+/-/_ characters."
msgstr ""
"Deze waarde mag alleen letters, cijfers en de tekens @/./+/-/_ bevatten."

#: user_admin.py:81
msgid "Unlocked selected usernames"
msgstr "Ontgrendel geselecteerde gebruikersnamen"

#: user_admin.py:82
msgid "Unlock usernames of users"
msgstr "Ontgrendel gebruikersnamen van gebruikers"

#: user_admin.py:88
#, python-format
msgid "%s users were (re)activated"
msgstr "%s gebruikers zijn ge(her)activeerd"

#: user_admin.py:89
msgid "(Re)activate users"
msgstr "(Her)activatie gebruikers"

#: user_admin.py:115
msgid "Set temporary password"
msgstr "Tijdelijk wachtwoord instellen"

#: utils.py:55
#, python-brace-format
msgid "Allowed characters: {chars}"
msgstr "Toegestane tekens: {chars}"

#: templates/admin/django_auth_policy/confirm_temporary_password.html:5
#: templates/admin/django_auth_policy/set_temporary_password.html:6
#: templates/admin/django_auth_policy/set_temporary_password.html:21
msgid "Temporary password"
msgstr "Tijdelijk wachtwoord"

#: templates/admin/django_auth_policy/confirm_temporary_password.html:7
msgid "Selected user(s)"
msgstr "Selecteer gebruiker(s)"

#: templates/admin/django_auth_policy/confirm_temporary_password.html:11
#: templates/admin/django_auth_policy/set_temporary_password.html:20
msgid "Email"
msgstr "E-mail"

#: templates/admin/django_auth_policy/confirm_temporary_password.html:15
msgid ""
"If you continue a temporary password will be set for\n"
"  these users. They will be required to change their password on first "
"usage.\n"
"  "
msgstr ""
"Als u doorgaat wordt er een tijdelijk wachtwoord in gesteld voor deze "
"gebruikers. Bij het eerst inloggen worden zij verplicht het wachtwoord aan "
"te passen."

#: templates/admin/django_auth_policy/confirm_temporary_password.html:21
#: templates/admin/django_auth_policy/set_temporary_password.html:32
msgid "Continue"
msgstr "Doorgaan"

#: templates/admin/django_auth_policy/confirm_temporary_password.html:22
msgid "Cancel"
msgstr "Annuleren"

#: templates/admin/django_auth_policy/set_temporary_password.html:8
msgid ""
"The temporary password(s) have been set. Now securely\n"
"  inform the user(s) about their new password."
msgstr ""
"De tijdelijke wachtwoord(en) zijn ingesteld. Informeer de gebruiker(s) over "
"hun nieuwe wachtwoord op een velige wijze."

#: templates/admin/django_auth_policy/set_temporary_password.html:10
msgid ""
"The user(s) will be required to change their password on\n"
"  first usage."
msgstr ""
"De gebruiker(s) zijn verplicht om hun wachtwoord aan te passen bij het "
"eerste gebruik."

#: templates/admin/django_auth_policy/set_temporary_password.html:17
msgid "User"
msgstr "Gebruiker"

#: templates/admin/django_auth_policy/set_temporary_password.html:23
msgid "Your temporary password"
msgstr "Uw tijdelijke wachtwoord"

#: templates/admin/django_auth_policy/set_temporary_password.html:25
msgid "Compose email"
msgstr "E-mail samenstellen"

#: tests/templates/change_password.html:4
msgid "Change password"
msgstr "Wachtwoord aanpassen"

#: tests/templates/change_password.html:6
msgid "Password policy"
msgstr "Wachtwoordbeleid"

#: tests/templates/change_password.html:12
msgid "Every password must have:"
msgstr "Elk wachtwoord bevat:"

#: tests/templates/change_password.html:20
#, python-format
msgid ""
"Passwords must be changed every\n"
"        %(max_password_age)s days."
msgstr "Elke %(max_password_age)s dagen moet een wachtwoord worden aangepast."
