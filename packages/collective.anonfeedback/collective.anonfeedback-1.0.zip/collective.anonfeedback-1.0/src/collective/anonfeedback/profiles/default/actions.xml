<?xml version="1.0"?>
<object name="portal_actions" xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <object name="site_actions" meta_type="CMF Action Category">
  <object name="give_feedback" meta_type="CMF Action" i18n:domain="collective.anonfeedback">
   <property name="title" i18n:translate="">Give Feedback</property>
   <property name="description" i18n:translate="Give anonymous feedback"></property>
   <property
      name="url_expr">string:${globals_view/navigationRootUrl}/@@feedback-form</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="collective.anonfeedback: Give Feedback"/>
   </property>
   <property name="visible">True</property>
  </object>
  <object name="view_feedback" meta_type="CMF Action" i18n:domain="collective.anonfeedback">
   <property name="title" i18n:translate="">View Feedback</property>
   <property name="description" i18n:translate="View site feedback"></property>
   <property
      name="url_expr">string:${globals_view/navigationRootUrl}/@@feedback-view</property>
   <property name="link_target"></property>
   <property name="icon_expr"></property>
   <property name="available_expr"></property>
   <property name="permissions">
    <element value="collective.anonfeedback: View Feedback"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
</object>