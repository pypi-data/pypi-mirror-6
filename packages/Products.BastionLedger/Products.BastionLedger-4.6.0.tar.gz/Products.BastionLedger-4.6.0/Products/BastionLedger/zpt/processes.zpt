<html tal:replace="structure context/manage_page_header"/>

    <span tal:replace="structure context/manage_tabs"/>

    <br/>

    <table>
       <tr tal:repeat="process python: context.portal_bastionledger.processesForLedger(context)">
          <td>
             <img src="" alt="" 
                  tal:attributes="src process/icon; alt process/title_or_id" />
          </td>
          <td>
             <a href="" tal:attributes="href string:${context/absolute_url}/${process/getId}/manage_main"
                tal:content="process/title_or_id"/>
             <span class="form-field" tal:content="process/Description"/>
          </td>
       </tr>
    </table>

<html tal:replace="structure context/manage_page_footer"/>



