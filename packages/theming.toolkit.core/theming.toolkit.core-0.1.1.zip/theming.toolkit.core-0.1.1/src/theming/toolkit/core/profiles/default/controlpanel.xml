<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="theming.toolkit.core"
    purge="False">

  <configlet
      action_id="propertyshelf_toolkit"
      appId="theming.toolkit.core"
      category="Products"
      condition_expr=""
      icon_expr="string:$portal_url/++resource++theming.toolkit.core/icon.png"
      title="Theming Toolset Settings"
      url_expr="string:${portal_url}/@@toolkit-controlpanel"
      visible="True"
      i18n:attributes="title">
    <permission>Plone Site Setup: Theming Toolkit</permission>
  </configlet>

</object>
