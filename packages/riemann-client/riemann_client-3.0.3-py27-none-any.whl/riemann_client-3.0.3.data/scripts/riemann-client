# EASY-INSTALL-ENTRY-SCRIPT: 'riemann-client==3.0.3','console_scripts','riemann-client'
__requires__ = 'riemann-client==3.0.3'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('riemann-client==3.0.3', 'console_scripts', 'riemann-client')()
    )
