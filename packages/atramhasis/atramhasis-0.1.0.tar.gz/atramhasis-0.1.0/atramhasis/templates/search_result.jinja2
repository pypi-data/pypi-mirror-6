{% extends 'base.jinja2' %}

{% block html_title %}Search{% endblock %}

{% block content %}


<h2>{% trans count=concepts|length %}There is {{ count }} result:{% pluralize %}There are {{ count }} results:{% endtrans %}</h2>

    <ul>
        {%- for c in concepts %}
         <li>{{ c.id }}: <a href="{{ request.route_path('concept', scheme_id= scheme_id, c_id = c.id) }}">{{ c.label }}</a></li>
        {%- endfor %}
    </ul>

{% endblock %}
