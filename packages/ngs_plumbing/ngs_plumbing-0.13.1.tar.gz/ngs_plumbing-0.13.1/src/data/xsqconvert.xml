<tool id="ngsp-xsqconvert-1201" name="XSQ-to-CSFASTA+QUAL" version="0.1" force_refresh_history="True">
<description>Extract data in XSQ files into CSFASTA + QUAL</description>
<version_string>xsqconvert --version</version_string>
<command>xsqconvert -C $input1</command>
<inputs>
  <param name="input1" type="data" format="xsq" label="XSQ file to convert"/>
</inputs>
<outputs>
  <data format="csfasta" name="output1" metadata_source="input1" label="${tool.name} on ${on_string}: converted CSFASTA">
  <data format="qual" name="output2" metadata_source="input1" label="${tool.name} on ${on_string}: converted QUAL">
</outputs>
<help>
  This tool extract sequencing data in a XSQ file into CSFASTA + QUAL files
</help>
</tool>