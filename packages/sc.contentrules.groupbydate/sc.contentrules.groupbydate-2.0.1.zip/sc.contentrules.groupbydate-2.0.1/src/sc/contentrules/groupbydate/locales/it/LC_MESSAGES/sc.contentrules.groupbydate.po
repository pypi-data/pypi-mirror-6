#: sc/contentrules/groupbydate/config.py:7
msgid " One level up"
msgstr "Un livello piú su"

#: sc/contentrules/groupbydate/config.py:8
msgid " Same folder of content"
msgstr "Stesso cartella del contenuto"

#: sc/contentrules/groupbydate/actions/groupbydate.py:204
msgid "A content rules action to move an item to a folder structure."
msgstr "Un'azione per regola di contenuto per spostare un oggetto in una gerarchia di cartelle."

#: ../configure.zcml
msgid "Action for content rules that creates a date-based folder structure."
msgstr "Azione per regola di contenuto che crea una struttura di cartelle basata su data."

#: sc/contentrules/groupbydate/actions/groupbydate.py:203
msgid "Add group by date folder action"
msgstr "Aggiungi azione 'raggruppa per cartelle data'"

#: sc/contentrules/groupbydate/interfaces.py:33
msgid "Base folder"
msgstr "Cartella base"

#: sc/contentrules/groupbydate/actions/groupbydate.py:86
msgid "Base folder ${target} does not exist."
msgstr "La cartella base ${target} non esiste"

#: sc/contentrules/groupbydate/interfaces.py:48
msgid "Choose hierarchy structure. Use strftime formating. e.g.: '%Y/%m/%d' to have 2011/11/17 or '%Y/%m' to have 2011/11"
msgstr "Scegli la gerarchia. Usa la formattazione di strftime. Es: '%Y/%m/%d' per avere '2011/11/17' o  '%Y/%m' per avere '2011/11'"

#: sc/contentrules/groupbydate/interfaces.py:34
msgid "Choose the base folder for the date hierarchy."
msgstr "Scegli la cartella base per la gerarchia di date."

#: sc/contentrules/groupbydate/interfaces.py:40
msgid "Container"
msgstr "Contenitore"

#: ../configure.zcml
msgid "Content rules: Date-based folder action"
msgstr "Regole di contenuto: azione cartella basata su data"

#: sc/contentrules/groupbydate/actions/groupbydate.py:219
msgid "Edit group by date action"
msgstr "Modifica l'azione 'raggruppa per data'"

#: sc/contentrules/groupbydate/interfaces.py:47
msgid "Hierarchy structure"
msgstr "Gerarchia cartelle"

#: sc/contentrules/groupbydate/actions/groupbydate.py:49
msgid "Move the item under ${base_folder} using ${structure} structure"
msgstr "Sposta l'oggetto sotto ${base_folder} usando la struttura ${structure}"

#: sc/contentrules/groupbydate/interfaces.py:41
msgid "Select the type of container in which the structure will be based."
msgstr "Seleziona il tipo di contenitore che fará da base alla gerarchia di cartelle."

