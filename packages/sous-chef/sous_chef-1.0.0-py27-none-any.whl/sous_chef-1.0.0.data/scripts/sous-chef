#!python
# EASY-INSTALL-ENTRY-SCRIPT: 'sous-chef==1.0.0','console_scripts','sous-chef'
__requires__ = 'sous-chef==1.0.0'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('sous-chef==1.0.0', 'console_scripts', 'sous-chef')()
    )
