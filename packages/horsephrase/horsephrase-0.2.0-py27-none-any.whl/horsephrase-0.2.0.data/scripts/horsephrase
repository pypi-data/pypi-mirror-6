#!python
# EASY-INSTALL-ENTRY-SCRIPT: 'horsephrase==0.2.0','console_scripts','horsephrase'
__requires__ = 'horsephrase==0.2.0'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('horsephrase==0.2.0', 'console_scripts', 'horsephrase')()
    )
