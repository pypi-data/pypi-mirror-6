<html tal:replace="structure context/manage_page_header"/>

    <span tal:replace="structure context/manage_tabs"/>

    <br/>

    <table border="0" cellpadding="2" cellspacing="0" width="100%"
           tal:define="events context/objectValues">
        <tr class="list-header" tal:condition="events ">
               <td width="5%"  class="list-item">Type</td>
               <td width="40%" class="list-item">Title</td>
               <td width="15%" class="list-item">Effective</td>
               <td width="15%" class="list-item">Actual</td>
               <td width="15%" class="list-item">Performed By</td>
               <td class="list-item">&nbsp;</td>
        </tr>
        <tr tal:repeat="item events"
            tal:attributes="class python: repeat['item'].odd() and 'row-normal' or 'row-hilite'">
               <td class="list-item"><img src="" tal:attributes="src item/icon"/></td>
               <td class="list-item">
                   <a href="" tal:attributes="href string:${item/getId}/manage_workspace"  
                              tal:content="item/title"/>
               </td>
               <td class="list-item"
                   tal:content="python: item.effective().strftime('%Y/%m/%d')"/>
               <td class="list-item" 
                   tal:content="python: item.bobobase_modification_time().strftime('%Y/%m/%d')"/>
               <td class="list-item" tal:content="item/Creator"/>
               <td class="list-item">&nbsp;</td>
        </tr>
    </table>

<html tal:replace="structure context/manage_page_footer"/>





