<html tal:replace="structure context/manage_page_header"/>
     <span tal:replace="structure python:context.manage_tabs(help_topic='receivables.stx',
                                                             help_product='BL')"/>

     <div class="system-msg" tal:condition="request/form/error | nothing" 
         tal:content="structure request/form/error"/>

      <form action="" tal:attributes="action request/URL1" method="post">
         <table width="100%" cellspacing="0" cellpadding="2" border="0">
              <tr>
                  <td class="form-label" tal:content="python:'Title'"/>
                  <td class="form-element">
                      <input type="text" name="title" size="35" value=""
                            tal:attributes="value container/title | nothing">
                  </td>
              </tr>
             <tr>
                  <td class="form-label" valign="top"
                      tal:content="python:'Group'"/>
                  <td class="form-element" colspan="3">
                      <select name="group:list" multiple size="15"> 
                            <span tal:repeat="account context/accountValues">
                                  <option value="" tal:attributes="value account/getId"
                                          tal:content="account/prettyTitle" selected
                                          tal:condition="python: account.getId() in container.group"/>
                                  <option value="" tal:attributes="value account/getId"
                                          tal:content="account/prettyTitle" 
                                          tal:condition="python: not account.getId() in container.group"/>
                            </span>
                      </select>
                  </td>
              </tr>
              <tr>
                 <td colspan=2>
                   <input type="submit" name="manage_edit:method" value=" Edit ">
                 </td>
              </tr>
        </table>
      </form>
<html tal:replace="structure context/manage_page_footer"/>




