<html>
 <head>
  <title>iframe</title>
  <link type="text/css" rel="stylesheet" tal:attributes="href context/globals/frontend.css/absolute_url" />
  <link type="text/css" rel="stylesheet" tal:attributes="href context/globals/kupu.css/absolute_url" />
  <meta http-equiv="Content-Type" content="text/html;charset=UTF-8" />
  <script type="text/javascript">
   function load_document() {
     var content_id = parent.location.search.match('content_id=(.*)')
     if (content_id.length == 2) {
	var content_id = content_id[1];
	var content_container = parent.opener.document.getElementById(content_id);
	var body = document.getElementsByTagName('body')[0];
	body.innerHTML = content_container.innerHTML;
      }
   }
  </script>
 </head>
 <body onload="load_document()"></body>
</html>