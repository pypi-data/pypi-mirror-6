<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <action-provider name="portal_actions" />
 <object name="object_buttons" meta_type="CMF Action Category">
  <object name="manage_zoomit" meta_type="CMF Action" i18n:domain="plone">
   <property name="title" i18n:translate="">Zoom.it Info</property>
   <property name="description" i18n:translate="">Manage Zoom.it Info</property>
   <property
      name="url_expr">string:$object_url/@@zoomit-updater</property>
   <property
      name="available_expr">exists:context/@@zoomit-updater</property>
   <property name="permissions">
    <element value="Modify portal content"/>
   </property>
   <property name="visible">True</property>
  </object>
 </object>
</object>
