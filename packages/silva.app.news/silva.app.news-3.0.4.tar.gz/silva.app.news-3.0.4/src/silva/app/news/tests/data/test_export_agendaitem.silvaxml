<?xml version="1.0" encoding="utf-8"?>
<silva xmlns="http://infrae.com/namespace/silva" {namespaces} silva_version="{version}">
  <folder id="export">
    <metadata>
      <set id="silva-content">
        <silva-content:maintitle>Export Folder</silva-content:maintitle>
        <silva-content:shorttitle/>
      </set>
      <set id="silva-extra">
        <silva-extra:comment/>
        <silva-extra:contactemail/>
        <silva-extra:contactname/>
        <silva-extra:content_description/>
        <silva-extra:creationtime>
          YYYY-MM-DDTHH:MM:SS
        </silva-extra:creationtime>
        <silva-extra:creator>author</silva-extra:creator>
        <silva-extra:keywords/>
        <silva-extra:language/>
        <silva-extra:lastauthor>author</silva-extra:lastauthor>
        <silva-extra:modificationtime>
          YYYY-MM-DDTHH:MM:SS
        </silva-extra:modificationtime>
        <silva-extra:subject/>
      </set>
      <set xmlns:silvasettings="http://infrae.com/namespace/metadata/silva-settings" id="silva-settings">
        <silvasettings:hide_from_tocs>
          do not hide
      </silvasettings:hide_from_tocs>
      </set>
    </metadata>
    <content>
      <silva-app-news:news_publication id="news">
        <metadata>
          <set id="silva-content">
            <silva-content:maintitle>News Publication</silva-content:maintitle>
            <silva-content:shorttitle/>
          </set>
          <set id="silva-extra">
            <silva-extra:comment/>
            <silva-extra:contactemail/>
            <silva-extra:contactname/>
            <silva-extra:content_description/>
            <silva-extra:creationtime>
              YYYY-MM-DDTHH:MM:SS
            </silva-extra:creationtime>
            <silva-extra:creator>author</silva-extra:creator>
            <silva-extra:keywords/>
            <silva-extra:language/>
            <silva-extra:lastauthor>author</silva-extra:lastauthor>
            <silva-extra:modificationtime>
              YYYY-MM-DDTHH:MM:SS
            </silva-extra:modificationtime>
            <silva-extra:subject/>
          </set>
          <set xmlns:silvasettings="http://infrae.com/namespace/metadata/silva-settings" id="silva-settings">
            <silvasettings:hide_from_tocs>
              hide
            </silvasettings:hide_from_tocs>
          </set>
          <set xmlns:snn-np-settings="http://infrae.com/namespace/metadata/snn-np-settings" id="snn-np-settings">
            <snn-np-settings:is_private>no</snn-np-settings:is_private>
          </set>
        </metadata>
        <content>
          <default>
            <silva-app-news:news_viewer hide_expired_events="True" id="index" number_is_days="False" number_to_show="25" number_to_show_archive="10">
              <metadata>
                <set id="silva-content">
                  <silva-content:maintitle>
                    News Publication
                  </silva-content:maintitle>
                  <silva-content:shorttitle>
                  </silva-content:shorttitle>
                </set>
                <set id="silva-extra">
                  <silva-extra:comment>
                  </silva-extra:comment>
                  <silva-extra:contactemail>
                  </silva-extra:contactemail>
                  <silva-extra:contactname>
                  </silva-extra:contactname>
                  <silva-extra:content_description>
                  </silva-extra:content_description>
                  <silva-extra:creationtime>
                    YYYY-MM-DDTHH:MM:SS
                  </silva-extra:creationtime>
                  <silva-extra:creator>
                    author
                  </silva-extra:creator>
                  <silva-extra:keywords>
                  </silva-extra:keywords>
                  <silva-extra:language>
                  </silva-extra:language>
                  <silva-extra:lastauthor>
                    author
                  </silva-extra:lastauthor>
                  <silva-extra:modificationtime>
                    YYYY-MM-DDTHH:MM:SS
                  </silva-extra:modificationtime>
                  <silva-extra:subject>
                  </silva-extra:subject>
                </set>
                <set xmlns:silvasettings="http://infrae.com/namespace/metadata/silva-settings" id="silva-settings">
                  <silvasettings:hide_from_tocs>
                    do not hide
                  </silvasettings:hide_from_tocs>
                </set>
              </metadata>
              <content>
                <silva-app-news:filters>
                  <silva-app-news:filter target="export/news/filter">
                  </silva-app-news:filter>
                </silva-app-news:filters>
              </content>
            </silva-app-news:news_viewer>
          </default>
          <silva-app-news:agenda_item id="event">
            <workflow>
              <version id="0">
                <status>public</status>
                <publication_datetime>YYYY-MM-DDTHH:MM:SS</publication_datetime>
                <expiration_datetime/>
              </version>
            </workflow>
            <content display_datetime="YYYY-MM-DDTHH:MM:SS"
                     subjects="all,other"
                     target_audiences="generic"
                     version_id="0">
              <metadata>
                <set id="silva-content">
                  <silva-content:maintitle>Some event</silva-content:maintitle>
                  <silva-content:shorttitle/>
                </set>
                <set id="silva-extra">
                  <silva-extra:comment/>
                  <silva-extra:contactemail/>
                  <silva-extra:contactname/>
                  <silva-extra:content_description/>
                  <silva-extra:creationtime>
                    YYYY-MM-DDTHH:MM:SS
                  </silva-extra:creationtime>
                  <silva-extra:creator>
                    author
                  </silva-extra:creator>
                  <silva-extra:keywords/>
                  <silva-extra:language/>
                  <silva-extra:lastauthor>
                    editor
                  </silva-extra:lastauthor>
                  <silva-extra:modificationtime>
                    YYYY-MM-DDTHH:MM:SS
                  </silva-extra:modificationtime>
                  <silva-extra:subject/>
                </set>
                <set xmlns:silvasettings="http://infrae.com/namespace/metadata/silva-settings" id="silva-settings">
                  <silvasettings:hide_from_tocs>
                    do not hide
                  </silvasettings:hide_from_tocs>
                </set>
              </metadata>
              <silva-app-news:occurrence
                  all_day="True"
                  end_datetime="YYYY-MM-DDTHH:MM:SS"
                  location="Rotterdam"
                  recurrence="FREQ=DAILY;UNTIL=20100910T123212Z"
                  start_datetime="YYYY-MM-DDTHH:MM:SS"
                  timezone_name="Europe/Amsterdam">
              </silva-app-news:occurrence>
              <silva-app-news:body>
                <silva-core-editor:text>
                  <html:p>
                    Good news!
                  </html:p>
                  <html:p>
                    I fixed the tests.
                  </html:p>
                </silva-core-editor:text>
              </silva-app-news:body>
            </content>
          </silva-app-news:agenda_item>
          <silva-app-news:news_filter id="filter"
                                      show_agenda_items="0"
                                      subjects="generic,all,other,root"
                                      target_audiences="generic,all,root">
            <metadata>
              <set id="silva-content">
                <silva-content:maintitle>
                  Filter for News Publication
                </silva-content:maintitle>
                <silva-content:shorttitle>
                </silva-content:shorttitle>
              </set>
              <set id="silva-extra">
                <silva-extra:comment>
                </silva-extra:comment>
                <silva-extra:contactemail>
                </silva-extra:contactemail>
                <silva-extra:contactname>
                </silva-extra:contactname>
                <silva-extra:content_description>
                </silva-extra:content_description>
                <silva-extra:creationtime>
                  YYYY-MM-DDTHH:MM:SS
                </silva-extra:creationtime>
                <silva-extra:creator>
                  author
                </silva-extra:creator>
                <silva-extra:keywords>
                </silva-extra:keywords>
                <silva-extra:language>
                </silva-extra:language>
                <silva-extra:lastauthor>
                  author
                </silva-extra:lastauthor>
                <silva-extra:modificationtime>
                  YYYY-MM-DDTHH:MM:SS
                </silva-extra:modificationtime>
                <silva-extra:subject>
                </silva-extra:subject>
              </set>
            </metadata>
            <content>
              <silva-app-news:sources>
                <silva-app-news:source target="export/news">
                </silva-app-news:source>
              </silva-app-news:sources>
              <silva-app-news:excludes>
              </silva-app-news:excludes>
            </content>
          </silva-app-news:news_filter>
        </content>
      </silva-app-news:news_publication>
    </content>
  </folder>
</silva>
