<div id="j01FilterLoadForm" class="j01FilterDialog" i18n:domain="j01">
  <form action="." method="post" enctype="multipart/form-data" class="edit-form"
        tal:attributes="action view/action;
                        name view/name;
                        id view/id">
    <div metal:use-macro="macro:header"></div>
    <div class="filterNames">
      <input type="radio" name="filterName" class="searchFilterNameRadio"
           tal:repeat="item view/filters"
           tal:replace="structure item" />
    </div>
    <div class="btns">
      <div class="left">
        <input tal:replace="structure view/actions/load/render" />
      </div>
      <div class="right">
        <input tal:replace="structure view/actions/remove/render" />
        <input tal:replace="structure view/actions/close/render" />
      </div>
    </div>
  </form>
</div>