<?xml version="1.0" encoding="utf-8"?>
<tryton>
    <data noupdate="1">
    
        <record id="linkedin_authorized_login" model="nereid.url_rule">
            <field name="rule">/auth/linkedin_authorized_login</field>
            <field name="endpoint">nereid.user.linkedin_authorized_login</field>
            <field name="sequence" eval="100" />
            <field name="http_method_get" eval="True"/>
            <field name="url_map" ref="nereid.default_url_map" />
        </record>
    
        <record id="linkedin_login" model="nereid.url_rule">
            <field name="rule">/auth/linkedin</field>
            <field name="endpoint">nereid.user.linkedin_login</field>
            <field name="sequence" eval="110" />
            <field name="http_method_get" eval="True"/>
            <field name="url_map" ref="nereid.default_url_map" />
        </record>

    </data>
</tryton>
