#!python
# EASY-INSTALL-ENTRY-SCRIPT: 'sous-chef==0.3.1','console_scripts','sous-chef-debug'
__requires__ = 'sous-chef==0.3.1'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('sous-chef==0.3.1', 'console_scripts', 'sous-chef-debug')()
    )
