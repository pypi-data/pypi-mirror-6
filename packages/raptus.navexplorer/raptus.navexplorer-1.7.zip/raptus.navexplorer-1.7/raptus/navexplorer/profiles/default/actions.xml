<?xml version="1.0" encoding="UTF-8"?>

<object name="portal_actions"
        meta_type="Plone Actions Tool"
        xmlns:i18n="http://xml.zope.org/namespaces/i18n">
    <object name="object" meta_type="CMF Action Category">
        <object name="open_navexplorer" meta_type="CMF Action" i18n:domain="raptus.navexplorer">
          <property name="title" i18n:translate="">Tree</property>
          <property name="description" i18n:translate="">Open navigation explorer</property>
          <property name="url_expr">string:${portal_url}/@@navexplorer_view</property>
          <property name="icon_expr"></property>
          <property name="available_expr"></property>
          <property name="permissions">
            <element value="raptus.navexplorer: View"/>
          </property>
          <property name="visible">True</property>
        </object>
    </object>
</object>