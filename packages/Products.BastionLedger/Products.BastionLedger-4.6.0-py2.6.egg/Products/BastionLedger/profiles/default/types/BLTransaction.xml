<?xml version="1.0"?>
<object name="BLTransaction" meta_type="Factory-based Type Information with dynamic views"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <property name="title">BLTransaction</property>
 <property name="description">General Ledger Transaction</property>
 <property name="content_icon">bltransaction.gif</property>
 <property name="content_meta_type">BLTransaction</property>
 <property name="product">BastionLedger</property>
 <property name="factory">addBLTransaction</property>
 <property name="immediate_view">bltransaction_edit_form</property>
 <property name="global_allow">False</property>
 <property name="filter_content_types">True</property>
 <property name="allowed_content_types"/>
 <property name="allow_discussion">False</property>
 <alias from="(Default)" to="bltransaction_view"/>
 <alias from="attach" to="blattachments_edit_form"/>
 <alias from="edit" to="bltransaction_edit_form"/>
 <alias from="view" to="bltransaction_view"/>
 <action title="View" action_id="view" category="object" condition_expr=""
    url_expr="string:${object_url}/view" visible="True">
  <permission value="View"/>
 </action>
 <action title="Edit" action_id="edit" category="object" condition_expr=""
    url_expr="string:${object_url}/edit" visible="True">
  <permission value="BastionLedger: Operate"/>
 </action>
 <action title="Attachments" action_id="attach" category="object"
    condition_expr="" url_expr="string:${object_url}/attach"
    visible="True">
  <permission value="BastionLedger: Operate"/>
 </action>
</object>
