            <td tal:content="view/version">4.0.1</td>
        </tr>
        <tr tal:condition="view/defaultpage">
            <td i18n:translate="">Default Page</td>
            <td tal:content="view/defaultpage">Artikel XY</td>
        </tr>
        <tr>
            <td i18n:translate="">E-mail from address</td>
            <td tal:content="view/emailfromaddress">info@raptus.com</td>
        </tr>
        <tr>
            <td i18n:translate="">E-mail from name</td>
            <td tal:content="view/emailfromname">info@raptus.com</td>
        </tr>
    </table>


</html>

