xed=512;yed=256;stp=32
size(xed,yed);colorrange(255);colormode(RGB)
var('noise',NUMBER,16.,0.,32.)
def draw():
  background(128,128,128)
  for y in range(0,yed/stp,1):
    for x in range(0,xed/stp,1):
      fill(255,255,255,128)
      beginpath(x*stp+random(0,noise),y*stp+random(0,noise))
      lineto(stp+x*stp+random(0,noise),y*stp+random(0,noise))
      lineto(stp+x*stp+random(0,noise),stp+y*stp+random(0,noise))
      lineto(x*stp+random(0,noise),stp+y*stp+random(0,noise))
      endpath()