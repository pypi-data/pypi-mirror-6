<?xml version="1.0"?>
<object name="BLPart" meta_type="Factory-based Type Information with dynamic views"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <property name="title"></property>
 <property name="description"> A unit of stock </property>
 <property name="content_icon">blpart.gif</property>
 <property name="content_meta_type">BLPart</property>
 <property name="product">BastionLedger</property>
 <property name="factory">addBLPart</property>
 <property name="immediate_view">bldublincore_edit_form</property>
 <property name="global_allow">False</property>
 <property name="filter_content_types">True</property>
 <property name="allowed_content_types">
  <element value="Document"/>
  <element value="File"/>
  <element value="Image"/>
 </property>
 <property name="allow_discussion">False</property>
 <alias from="(Default)" to="@@blpart_view"/>
 <alias from="edit" to="blpart_contentedit_form"/>
 <alias from="pricing" to="blpart_edit_form"/>
 <alias from="view" to="@@blpart_view"/>
 <action title="View" action_id="view" category="object" condition_expr=""
    url_expr="string:${object_url}/view" visible="True">
  <permission value="View"/>
 </action>
 <action title="Edit" action_id="edit" category="object" condition_expr=""
    url_expr="string:${object_url}/edit" visible="True">
  <permission value="Modify portal content"/>
 </action>
 <action title="Pricing" action_id="pricing" category="object"
    condition_expr="" url_expr="string:${object_url}/pricing"
    visible="True">
  <permission value="BastionLedger: Operate"/>
 </action>
</object>
