#!python
# EASY-INSTALL-ENTRY-SCRIPT: 'twitter==1.13.1','console_scripts','twitter-archiver'
__requires__ = 'twitter==1.13.1'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('twitter==1.13.1', 'console_scripts', 'twitter-archiver')()
    )
