<?xml version="1.0"?>

<form>
  <title>Parameters form</title>
  <row_length>4</row_length>
  <name>form</name>
  <action></action>
  <method>POST</method>
  <enctype></enctype>
  <encoding>UTF-8</encoding>
  <stored_encoding>ISO-8859-1</stored_encoding>
