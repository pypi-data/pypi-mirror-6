size(400,400)
speed(20)

xcount = 10
ycount = 10

def draw():
	for i in range(xcount):
		for j in range(ycount):
			#transform(CORNER)
			translate(WIDTH/xcount*i, HEIGHT/ycount*j)
			#translate(200,200)			
			strokewidth(1)
			stroke(0)
			line(WIDTH/xcount*i,HEIGHT/ycount*j,MOUSEX-WIDTH/xcount*i,MOUSEY-HEIGHT/ycount*j)
			reset()
