<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html tal:omit-tag=""
      lang="en"
      xml:lang="en"
      xmlns="http://www.w3.org/1999/xhtml"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:i18n="http://xml.zope.org/namespaces/i18n"
      i18n:domain="silva_document"
      tal:define="global vein string:editor;
                  model request/model;
                  no_empty_feedback python:True;
                  is_editable model/get_unapproved_version;">

  <metal:block use-macro="here/macro_tab_edit/macros/editor">

    <metal:block fill-slot="editor_css">
      <tal:condition condition="is_editable">
        <link href="kupustyles.css"
              type="text/css"
              rel="stylesheet"
              tal:attributes="href here/kupustyles.css/absolute_url"
              />
      </tal:condition>
    </metal:block>

    <metal:block fill-slot="script">
      <metal:macro use-macro="here/service_kupu_silva/kupumacros/macros/jsincludes" />
    </metal:block>
    <metal:block fill-slot="editor_content">
      <metal:macro use-macro="here/service_kupu_silva/kupumacros/macros/body" />
    </metal:block>

  </metal:block>
</html>
