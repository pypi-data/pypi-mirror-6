  <groups>
    <group>
      <title>Default</title>
      <fields>

      <field><id>google_calendar_account</id> <type>StringField</type>
        <values>
          <alternate_name></alternate_name>
          <css_class></css_class>
          <default>@GMail.com</default>
          <description>ID of your Google Calendar/Agenda account. eg: mycompany@gmail.com</description>
          <display_maxwidth></display_maxwidth>
          <display_width type="int">20</display_width>
          <enabled type="int">1</enabled>
          <external_validator></external_validator>
          <extra></extra>
          <hidden type="int">0</hidden>
          <max_length></max_length>
          <required type="int">1</required>
          <title>Google Calendar Account</title>
          <truncate type="int">0</truncate>
          <unicode type="int">0</unicode>
          <whitespace_preserve type="int">0</whitespace_preserve>
        </values>
        <tales>
        </tales>
        <messages>
          <message name="external_validator_failed">The input failed the external validator.</message>
          <message name="required_not_found">Input is required but no input given.</message>
          <message name="too_long">Too much input was given.</message>
        </messages>
      </field>
      <field><id>google_calendar_type</id> <type>RadioField</type>
        <values>
          <alternate_name></alternate_name>
          <css_class></css_class>
          <default>Month</default>
          <description>The type of google calendar to display</description>
          <enabled type="int">1</enabled>
          <external_validator></external_validator>
          <extra_item></extra_item>
          <first_item type="int">1</first_item>
          <hidden type="int">0</hidden>
          <items type="list">[('Month', 'MONTH'), ('Agenda', 'AGENDA')]</items>
          <orientation>vertical</orientation>
          <required type="int">1</required>
          <title>Google Calendar Type</title>
          <unicode type="int">0</unicode>
          <whitespace_preserve type="int">0</whitespace_preserve>
        </values>
        <tales>
        </tales>
        <messages>
          <message name="external_validator_failed">The input failed the external validator.</message>
          <message name="required_not_found">Input is required but no input given.</message>
          <message name="unknown_selection">You selected an item that was not in the list.</message>
        </messages>
      </field>
      <field><id>calendar_background_color</id> <type>StringField</type>
        <values>
          <alternate_name></alternate_name>
          <css_class></css_class>
          <default>FFFFFF</default>
          <description>Enter the six digits of the hexadecimal color code</description>
          <display_maxwidth type="int">6</display_maxwidth>
          <display_width type="int">20</display_width>
          <enabled type="int">1</enabled>
          <external_validator></external_validator>
          <extra></extra>
          <hidden type="int">0</hidden>
