#!python
# EASY-INSTALL-ENTRY-SCRIPT: 'check-manifest==0.18','console_scripts','check-manifest'
__requires__ = 'check-manifest==0.18'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('check-manifest==0.18', 'console_scripts', 'check-manifest')()
    )
