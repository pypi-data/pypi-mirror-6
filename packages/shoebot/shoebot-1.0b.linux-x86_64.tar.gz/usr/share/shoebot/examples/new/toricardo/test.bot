size(400,400)
background(1,1,1)

def thingie(x,y):
	fill(random(0.5,1),random(0.5),random(0.5))
	strokewidth(1)
	stroke(0.5)
	transform(CORNER)
	translate(x,y)
	rotate(random(360))	
	line(10,0,50,0)
	text(chr(random(97,122)),0,0)
	translate(50,0)
	text("a",0,0)
	reset()

for i in range(20):
	thingie(random(WIDTH),random(HEIGHT))
