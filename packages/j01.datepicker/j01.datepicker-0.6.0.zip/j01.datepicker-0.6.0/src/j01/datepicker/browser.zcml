    xmlns="http://namespaces.zope.org/browser"
    xmlns:z3c="http://namespaces.zope.org/z3c"
    i18n_domain="p01">

  <resourceDirectory
      name="j01DatePickerImages"
      directory="img"
      layer="zope.publisher.interfaces.http.IHTTPRequest"
      />

  <zrt-resource
      name="j01.datepicker.css"
      file="css/j01.datepicker.css"
      layer="zope.publisher.interfaces.http.IHTTPRequest"
      />

  <resource
      name="datepicker.js"
      file="js/datepicker.js"
      layer="zope.publisher.interfaces.http.IHTTPRequest"
      />

</configure>