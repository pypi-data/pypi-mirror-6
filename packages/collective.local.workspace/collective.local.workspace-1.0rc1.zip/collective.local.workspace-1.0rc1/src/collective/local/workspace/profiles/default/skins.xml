<?xml version="1.0"?>
<object name="portal_skins" meta_type="Plone Skins Tool" allow_any="False"
   cookie_persistence="False"
   request_varname="plone_skin"
   based-on="Plone Default">

  <object name="collective_local_workspace"
          meta_type="Filesystem Directory View"
          directory="collective.local.workspace:skins/collective_local_workspace" />

  <skin-path name="*">
    <layer name="collective_local_workspace" insert-after="custom" />
  </skin-path>

</object>
