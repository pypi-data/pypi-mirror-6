<configure    xmlns:zcml="http://qrcodenamespaces.zope.org/zcml"    xmlns:browser="http://namespaces.zope.org/browser"    xmlns:plone="http://namespaces.plone.org/plone"    i18n_domain="medialog.qrcode">

           <browser:page		for="*"        name="qrcode"		class="medialog.qrcode.browser.qrviews.QRImage"        permission="zope2.View"
        />

        <browser:page        for="*"        name="qrsummary"        class="medialog.qrcode.browser.qrviews.QrSummaryView"        template="qrsummaryview.pt"        allowed_interface="medialog.qrcode.browser.qrviews.IQrSummaryView"        permission="zope2.View"    /></configure>