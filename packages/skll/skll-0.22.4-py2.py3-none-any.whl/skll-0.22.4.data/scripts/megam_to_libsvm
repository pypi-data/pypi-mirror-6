#!python
# EASY-INSTALL-ENTRY-SCRIPT: 'skll==0.22.4','console_scripts','megam_to_libsvm'
__requires__ = 'skll==0.22.4'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('skll==0.22.4', 'console_scripts', 'megam_to_libsvm')()
    )
