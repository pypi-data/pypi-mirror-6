{% extends "sacrud/types/SacrudBase.jinja2" %}

{% block sacrud_column %}
  <input type="file"
        class="sacrud_filestore {{ css_styles }}"
        id="sacrud_filestore_{{ col.name }}"
        name="{{ col.name }}"
        value="{{ value }}"
        />

  {% if obj and value %}
    <a href="{{ value }}">file</a>
  {% endif %}
{% endblock %}
