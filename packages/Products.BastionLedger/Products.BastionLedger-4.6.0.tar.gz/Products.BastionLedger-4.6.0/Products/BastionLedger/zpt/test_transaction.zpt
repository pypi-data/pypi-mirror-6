<html tal:replace="structure context/manage_page_header"
      tal:define="global txn context;
                  global entries txn/entries"/>

    <span tal:replace="structure context/manage_tabs"/>

       <br/>
       <table border="0" cellpadding="2" cellspacing="0">
           <tr>
               <span tal:condition="txn/reference" tal:omit-tag="">
                    <td class="form-label">Reference</td>
                    <td class="form-element">
                        <a href="" tal:attributes="href txn/referenceUrl"
                                   tal:content="txn/referenceId"/>
                    </td>
               </span>
               <td class="form-label">Dated</td>
               <td class="form-element" tal:content="txn/effective"/>
               <td>&nbsp;</td>
               <td class="form-label">Entered By</td>
               <td class="form-element" tal:content="txn/entered_by"/>
               <td class="form-label">Status</td>
               <td class="form-element" tal:content="txn/status"/>
           </tr>
       </table>
       <br/>
       <form action="" method="post" tal:attributes="action request/URL1">
       <table border="0" cellpadding="2" cellspacing="0" width="100%" tal:condition="entries">
           <tr class="list-header">
               <td width="5%"  class="list-item">Type</td>
               <td width="30%" class="list-item">Account</td>
               <td width="30%" class="list-item">Description</td>
               <td width="10%" class="list-item" align="right">Debit</td>
               <td width="10%" class="list-item" align="right">Credit</td>
     	       <td width="5%"  class="list-item">&nbsp</td>
           </tr>
           <tr tal:repeat="item entries">
               <td class="list-item">
                  <a href="" tal:attributes="href python:'%s/manage_workspace' % item[0]">
                      <img src="" border="0" tal:attributes="src python:item[1].icon"/>
                  </a>
               </td>
               <td class="list-item">
                   <a href="" tal:define="acct python: item[1].Account()"
                              tal:attributes="href string:${acct/absolute_url}/manage_workspace"
                              tal:content="acct/prettyTitle"/>
               </td>
               <td class="list-item">
                   <a href="" tal:attributes="href python: '%s/manage_workspace' % item[0]"
                              tal:content="python: item[1].title"/>
               </td>
               <span tal:condition="python: item[1].isDebit()" tal:omit-tag="">
                  <td class="list-item" align="right" tal:content="python: item[1].absAmount()"/>
                  <td>&nbsp;</td>
               </span>
               <span tal:condition="python: not item[1].isDebit()" tal:omit-tag="">
                  <td>&nbsp;</td>
                  <td class="list-item" align="right" tal:content="python: item[1].absAmount()"/>
               </span>
               <td>&nbsp;</td>
           </tr>
           <tr class="list-header">
               <td width="10%" class="list-item" align="right" colspan="4">
                  <strong tal:content="txn/debitTotal"/></td>
               <td width="10%" class="list-item" align="right">
                  <strong tal:content="txn/creditTotal"/></td>
     	       <td width="5%"  class="list-item">&nbsp</td>
           </tr>
       </table>
       </form>
<html tal:replace="structure context/manage_page_footer"/>





