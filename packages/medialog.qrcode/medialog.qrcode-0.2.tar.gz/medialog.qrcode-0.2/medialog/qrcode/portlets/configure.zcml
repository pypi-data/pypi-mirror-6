<configure    xmlns:zcml="http://qrcodenamespaces.zope.org/zcml"    xmlns:browser="http://namespaces.zope.org/browser"    xmlns:plone="http://namespaces.plone.org/plone"    i18n_domain="medialog.qrcode">

           <!-- Register the portlet -->     <!-- If the portlet has no configurable parameters, you can remove          the EditForm declaration in qrportlet.py and delete          the 'editview' attribute from this statement.       -->     <plone:portlet         name="medialog.qrcode.QRPortlet"         interface=".qrportlet.IQRPortlet"         assignment=".qrportlet.Assignment"         view_permission="zope2.View"         renderer=".qrportlet.Renderer"         addview =".qrportlet.AddForm"         editview=".qrportlet.EditForm"         />    </configure>