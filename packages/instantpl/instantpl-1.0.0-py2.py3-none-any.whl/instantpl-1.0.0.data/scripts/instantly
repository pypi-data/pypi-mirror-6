#!python
# EASY-INSTALL-ENTRY-SCRIPT: 'instantpl==1.0.0','console_scripts','instantly'
__requires__ = 'instantpl==1.0.0'
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.exit(
        load_entry_point('instantpl==1.0.0', 'console_scripts', 'instantly')()
    )
