<?xml version="1.0"?>
<object
    name="portal_controlpanel"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n"
    i18n:domain="collective.flexitopic"
    purge="False">

    <configlet
        title="Slidehare"
        action_id="slidesharectlpanel"
        appId="collective.slideshare"
        category="Products"
        condition_expr=""
        url_expr="string:${portal_url}/@@slideshare-settings.html"
