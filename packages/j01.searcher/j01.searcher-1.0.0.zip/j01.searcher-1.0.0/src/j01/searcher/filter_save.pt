<div id="j01FilterSaveForm" class="j01FilterDialog" i18n:domain="j01">
  <form action="." method="post" enctype="multipart/form-data" class="edit-form"
        tal:attributes="action view/action;
                        name view/name;
                        id view/id">
    <div metal:use-macro="macro:header"></div>
    <tal:block define="widget nocall:view/widgets/name">
      <div metal:use-macro="macro:widget-row" />
    </tal:block>
    <div class="btns">
      <div class="left">
        <input tal:replace="structure view/actions/save/render" />
      </div>
      <div class="right">
        <input tal:replace="structure view/actions/close/render" />
      </div>
    </div>
  </form>
</div>