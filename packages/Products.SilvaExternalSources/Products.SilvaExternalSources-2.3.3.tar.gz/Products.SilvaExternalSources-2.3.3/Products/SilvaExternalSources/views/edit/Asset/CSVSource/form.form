<?xml version="1.0" encoding="UTF-8"?>

<form>
  <title></title>
  <name>form</name>
  <action></action>
  <enctype></enctype>
  <method>POST</method>

  <i18n_domain>silva_external_sources</i18n_domain>
  <i18n>form</i18n>

  <groups>
    <group>
      <title>default</title>
      <fields>

      <field><id>csv_title</id> <type>StringField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <display_width type="int">41</display_width>
          <unicode type="int">1</unicode>
          <max_length></max_length>
          <external_validator></external_validator>
          <display_maxwidth></display_maxwidth>
          <required type="int">0</required>
          <css_class>store</css_class>
          <default></default>
          <title>title</title>
          <truncate type="int">0</truncate>
          <description>Optional, but useful.</description>
          <extra></extra>
        </values>
        <tales>
          <default>python:form.REQUEST.model.get_title()</default>
        </tales>
        <messages>
          <message name="external_validator_failed">Sorry, the form content failed validation.</message>
          <message name="required_not_found">Input is required but no input given.</message>
          <message name="too_long">Too much input was given.</message>
        </messages>
      </field>
      <field><id>csv_description</id> <type>TextAreaField</type>
        <values>
          <alternate_name></alternate_name>
          <hidden type="int">0</hidden>
          <display_width type="int">41</display_width>
          <height type="int">2</height>
          <unicode type="int">1</unicode>
          <max_length></max_length>
          <external_validator></external_validator>
          <display_maxwidth></display_maxwidth>
          <required type="int">0</required>
          <css_class>store</css_class>
          <default></default>
          <title>description</title>
          <truncate type="int">0</truncate>
          <description>Optional, but useful.</description>
          <extra></extra>
        </values>
        <tales>
          <default>python:form.REQUEST.model.get_description()</default>
        </tales>
        <messages>
          <message name="external_validator_failed">Sorry, the form content failed validation.</message>
          <message name="required_not_found">Input is required but no input given.</message>
          <message name="too_long">Too much input was given.</message>
        </messages>
      </field>
      </fields>
    </group>
  </groups>
</form>
