<?xml version="1.0"?>
<object name="BLPayroll" meta_type="Factory-based Type Information with dynamic views"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <property name="title"></property>
 <property name="description">Payroll System and Ledger</property>
 <property name="content_icon">blpayroll.gif</property>
 <property name="content_meta_type">BLPayroll</property>
 <property name="product">BastionLedger</property>
 <property name="factory">addBLPayroll</property>
 <property name="immediate_view">folder_listing</property>
 <property name="global_allow">False</property>
 <property name="filter_content_types">True</property>
 <property name="allowed_content_types">
  <element value="BLEmployee"/>
  <element value="BLSubsidiaryTransaction"/>
  <element value="BLTimesheetSlot"/>
 </property>
 <property name="allow_discussion">False</property>
 <alias from="(Default)" to="blpayroll_view"/>
 <alias from="edit" to="blledger_props_form"/>
 <alias from="accounts" to="blledger_view"/>
 <alias from="transactions" to="bltransactions_view"/>
 <alias from="processes" to="blprocesses_view"/>
 <alias from="reports" to="blreports_view"/>
 <alias from="timesheeting" to="blpayroll_timesheeting_form"/>
 <alias from="super" to="blpayroll_superannuation_summary"/>
 <alias from="view" to="blpayroll_view"/>
 <action title="View" action_id="view" category="object" condition_expr=""
    url_expr="string:${object_url}/view" visible="True">
  <permission value="View"/>
 </action>
 <action title="Accounts" action_id="accounts" category="object"
    condition_expr="" url_expr="string:${object_url}/accounts"
    visible="True">
  <permission value="View"/>
 </action>
 <action title="Transactions" action_id="transactions" category="object"
    condition_expr="" url_expr="string:${object_url}/transactions"
    visible="True">
  <permission value="View"/>
 </action>
 <action title="Processes" action_id="processes" category="object"
    condition_expr="" url_expr="string:${object_url}/processes"
    visible="True">
  <permission value="View"/>
 </action>
 <action title="Reports" action_id="reports" category="object"
    condition_expr="" url_expr="string:${object_url}/reports"
    visible="True">
  <permission value="View"/>
 </action>
 <action title="Super" action_id="super" category="object" condition_expr=""
    url_expr="string:${object_url}/super"
    visible="True">
  <permission value="View"/>
 </action>
 <action title="Edit" action_id="edit" category="object" condition_expr=""
    url_expr="string:${object_url}/edit" visible="True">
  <permission value="Manage properties"/>
 </action>
 <action title="Timesheeting" action_id="timesheeting" category="object" condition_expr=""
    url_expr="string:${object_url}/timesheeting" visible="True">
  <permission value="Manage properties"/>
 </action>
</object>
