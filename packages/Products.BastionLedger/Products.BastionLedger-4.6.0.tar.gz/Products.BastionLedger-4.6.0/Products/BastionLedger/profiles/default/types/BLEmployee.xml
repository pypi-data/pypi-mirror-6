<?xml version="1.0"?>
<object name="BLEmployee" meta_type="Factory-based Type Information with dynamic views"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">
 <property name="title"></property>
 <property name="description"> Employee Account.</property>
 <property name="content_icon">blemployee.gif</property>
 <property name="content_meta_type">BLEmployee</property>
 <property name="product">BastionLedger</property>
 <property name="factory">addBLEmployee</property>
 <property name="immediate_view">blaccount_view</property>
 <property name="global_allow">False</property>
 <property name="filter_content_types">True</property>
 <property name="allowed_content_types">
  <element value="BLEntryTemplate"/>
 </property>
 <property name="allow_discussion">False</property>
 <alias from="(Default)" to="blaccount_view"/>
 <alias from="account" to="blaccount_edit_form"/>
 <alias from="edit" to="blemployee_edit_form"/>
 <alias from="settings" to="blemployee_settings_form"/>
 <alias from="timesheets" to="blemployee_timesheets"/>
 <alias from="deductions" to="blemployee_deductions"/>
 <alias from="attach" to="blattachments_edit_form"/>
 <alias from="taxgroups" to="blaccount_taxgroups_form"/>
 <alias from="view" to="blaccount_view"/>
 <action title="View" action_id="view" category="object" condition_expr=""
    url_expr="string:${object_url}/view" visible="True">
  <permission value="View"/>
 </action>
 <action title="Payslips" action_id="payslips" category="object"
    condition_expr="" url_expr="string:${object_url}/blemployee_payslips"
    visible="True">
  <permission value="View"/>
 </action>
 <action title="Edit" action_id="edit" category="object" condition_expr=""
    url_expr="string:${object_url}/edit" visible="True">
  <permission value="View"/>
 </action>
 <action title="Account" action_id="account" category="object"
    condition_expr="" url_expr="string:${object_url}/account"
    visible="True">
  <permission value="BastionLedger: Operate"/>
 </action>
 <action title="Settings" action_id="settings" category="object"
    condition_expr="" url_expr="string:${object_url}/settings"
    visible="True">
  <permission value="BastionLedger: Operate"/>
 </action>
 <action title="Timesheets" action_id="timesheets" category="object"
    condition_expr="" url_expr="string:${object_url}/timesheets"
    visible="True">
  <permission value="View"/>
 </action>
 <action title="Deductions" action_id="deductions" category="object"
    condition_expr="" url_expr="string:${object_url}/deductions"
    visible="True">
  <permission value="View"/>
 </action>
 <action title="Tax Groups" action_id="taxgroups" category="object" condition_expr=""
    url_expr="string:${object_url}/taxgroups" visible="True">
  <permission value="BastionLedger: Manage"/>
 </action>
 <action title="Attachments" action_id="attach" category="object"
    condition_expr="" url_expr="string:${object_url}/attach"
    visible="True">
  <permission value="BastionLedger: Operate"/>
 </action>
</object>
